{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pcoulibaly-formula1-project"
		},
		"datakind_test_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'datakind_test_ls'"
		},
		"ls_formula1_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_formula1_databricks'"
		},
		"ls_formula1_storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_formula1_storage'"
		},
		"ls_injectionsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_injectionsa'"
		},
		"ls_IPC_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://map.ipcinfo.org/api/public/"
		},
		"ls_fewsData_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fdw.fews.net/api/marketpricefacts/"
		},
		"ls_fewsDataKenFoodSecurity_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fdw.fews.net/api/ipcpackage/?country_code=KE&collection_date="
		},
		"ls_fewsDataSomFoodSecurity_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fdw.fews.net/api/ipcpackage/?country_code=SO&collection_date="
		},
		"ls_formula1_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0726-155654-d94drhe8"
		},
		"ls_hotosm_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://export.hotosm.org/downloads/"
		},
		"ls_humData_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.humdata.org/dataset/"
		},
		"trigger_every_15_minutes_properties_pipeline_parameters_FolderPath_Source": {
			"type": "string",
			"defaultValue": "datakind"
		},
		"trigger_every_15_minutes_properties_pipeline_parameters_Directory_Source": {
			"type": "string",
			"defaultValue": "raw"
		},
		"trigger_every_15_minutes_properties_pipeline_parameters_FolderPath_Destination": {
			"type": "string",
			"defaultValue": "datakind"
		},
		"trigger_every_15_minutes_properties_pipeline_parameters_Directory_Destination": {
			"type": "string",
			"defaultValue": "target"
		},
		"trigger_every_15_minutes_properties_pipeline_parameters_LastModified_From": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"trigger_every_15_minutes_properties_pipeline_parameters_LastModified_To": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pcoulibaly-formula1-project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datakind_test_ls_connectionString": {
						"value": "[parameters('datakind_test_ls_connectionString')]"
					},
					"ls_formula1_databricks_accessToken": {
						"value": "[parameters('ls_formula1_databricks_accessToken')]"
					},
					"ls_formula1_storage_connectionString": {
						"value": "[parameters('ls_formula1_storage_connectionString')]"
					},
					"ls_injectionsa_connectionString": {
						"value": "[parameters('ls_injectionsa_connectionString')]"
					},
					"ls_IPC_properties_typeProperties_url": {
						"value": "[parameters('ls_IPC_properties_typeProperties_url')]"
					},
					"ls_fewsData_properties_typeProperties_url": {
						"value": "[parameters('ls_fewsData_properties_typeProperties_url')]"
					},
					"ls_fewsDataKenFoodSecurity_properties_typeProperties_url": {
						"value": "[parameters('ls_fewsDataKenFoodSecurity_properties_typeProperties_url')]"
					},
					"ls_fewsDataSomFoodSecurity_properties_typeProperties_url": {
						"value": "[parameters('ls_fewsDataSomFoodSecurity_properties_typeProperties_url')]"
					},
					"ls_formula1_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_formula1_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_hotosm_properties_typeProperties_url": {
						"value": "[parameters('ls_hotosm_properties_typeProperties_url')]"
					},
					"ls_humData_properties_typeProperties_url": {
						"value": "[parameters('ls_humData_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "pcoulibaly-formula1-project_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'pcoulibaly-formula1-project_ArmTemplate_0')]"
			]
		},
		{
			"name": "pcoulibaly-formula1-project_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger_every_15_minutes_properties_pipeline_parameters_FolderPath_Source": {
						"value": "[parameters('trigger_every_15_minutes_properties_pipeline_parameters_FolderPath_Source')]"
					},
					"trigger_every_15_minutes_properties_pipeline_parameters_Directory_Source": {
						"value": "[parameters('trigger_every_15_minutes_properties_pipeline_parameters_Directory_Source')]"
					},
					"trigger_every_15_minutes_properties_pipeline_parameters_FolderPath_Destination": {
						"value": "[parameters('trigger_every_15_minutes_properties_pipeline_parameters_FolderPath_Destination')]"
					},
					"trigger_every_15_minutes_properties_pipeline_parameters_Directory_Destination": {
						"value": "[parameters('trigger_every_15_minutes_properties_pipeline_parameters_Directory_Destination')]"
					},
					"trigger_every_15_minutes_properties_pipeline_parameters_LastModified_From": {
						"value": "[parameters('trigger_every_15_minutes_properties_pipeline_parameters_LastModified_From')]"
					},
					"trigger_every_15_minutes_properties_pipeline_parameters_LastModified_To": {
						"value": "[parameters('trigger_every_15_minutes_properties_pipeline_parameters_LastModified_To')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'pcoulibaly-formula1-project_ArmTemplate_1')]"
			]
		}
	]
}